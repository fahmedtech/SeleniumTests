{ "store": {
    "book": [ 
      { "category": "reference",
        "author": "Nigel Rees",
        "title": "Sayings of the Century",
        "price": 8.95
      },
      { "category": "fiction",
        "author": "Evelyn Waugh",
        "title": "Sword of Honour",
        "price": 12.99
      },
      { "category": "fiction",
        "author": "Herman Melville",
        "title": "Moby Dick",
        "isbn": "0-553-21311-3",
        "price": 8.99
      },
      { "category": "fiction",
        "author": "J. R. R. Tolkien",
        "title": "The Lord of the Rings",
        "isbn": "0-395-19395-8",
        "price": 22.99
      }
    ],
    "bicycle": {
      "color": "red",
      "price": 19.95
    }
  }
}

//json viewer http://jsonviewer.stack.hu/
//using expressions https://jsonpath.curiousconcept.com/
$..author
$.store.book[0].author
$.store.book[*].author
$.store.book[0]
$.store.book[:1].author  -- first 2 books author

//using queries to find results
$.store.book[?(@.price)] -- get all that has price
$.store.book[?(@.category="reference")]
$.store.book[?(@.price>20)]

$.store.book[?(@.title="Sword of Honour")]		-- find book with title "Sword of Honour"
$.store.bicycle.price  							-- find bicycle price
$.store.book[-3:]								-- find last three books in store